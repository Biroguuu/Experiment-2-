#include <iostream>
#include <iomanip>
#include <conio.h>


using namespace std;

int main()
{
    int rows, columns;
    cout<<"Number of Rows: ";
    cin>>rows;
    cout<<"Number of Columns: ";
    cin>>columns;
    
    for (int x = 1; x <= rows; x++)
   
    {
       
	    for (int y = 1; y <= columns; y++)
        {
            cout << " * ";
        }
        
		cout<<endl;
    }
    
	
_getch();
	
return 0;


}
