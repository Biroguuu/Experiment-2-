#include <iostream>
#include <iomanip>
#include <conio.h>

using namespace std;

int main()
{
   
   int n;

    cout << "Enter a Number: "; cin >> n;
 	
	  cout << "Multiplication table of: " << n << endl;

    for (int i = 1; i <= 10; ++i) 
	{
	    cout << n << " x " << i << " = " << n * i << endl;
  }
  
	 _getch();
	
   return 0;
}
